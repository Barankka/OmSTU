
<table class="table">
    <tr>
        <td>
            <h4>Publication title</h4>
        </td>
        <td>
            <h4>Date</h4>
        </td>
        <td>
            <h4>User</h4>
        </td>
        <td>
            <h4>Rating</h4>
        </td>
        <td>
            <h4>
                Theme
            </h4>
        </td>
    </tr>
    @foreach (var item in ViewBag.publications)
    {
    <tr>
        <td>
            @Html.ActionLink((string)item.name, "FilterByPublication", "comments", new { id = item.id }, null)
        </td>
        <td>
            @Html.Raw((DateTime)item.date)
        </td>
        <td>
            @Html.ActionLink((string)item.users.name, "Details", "users", new { id = item.id }, null)
        </td>
        <td>
            @item.rating
        </td>
        <td>
            @item.themes.name
        </td>
    </tr>
    }
</table>
